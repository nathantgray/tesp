// This file is an example of the glm_config.json5 that is used to
// configure the process of populating a feeder.

{
    // RECS
    "use_recs": true,
    "recs_exist": true,
    "in_file_recs_income_level": '${TESPDIR}/data/feeders/RECSwIncomeLvl.csv',
    "in_file_residential_meta": '../data/DSOT_residential_metadata.json',
    "out_file_residential_meta": '../data/RECS_residential_metadata.json',
    "out_file_hvac_set_point": '../data/hvac_setpt_RECS.json',
    "sample": {
        'state': ["TX"],
        'housing_density': ['No_DSO_Type'],   //'No_DSO_Type', U, R, C possibly = S
        'income_level': ['Low', 'Middle', 'Upper']
    },
    "state": "TX",
    "res_dso_type": "No_DSO_Type",
    "income_level": ['Low', 'Middle', 'Upper'],
    "bin_size_threshold": 100,
    "climate_zone": null,
    "wh_shift": 0,

    // general
    "in_file_glm": "", // REQUIRED
    "out_file_glm": "test.glm", // REQUIRED
    "case_name": "MyCase", // REQUIRED
    "substation_name": "MySub", // REQUIRED
    "starttime": "2023-01-01 00:00:00", // REQUIRED: format "2023-01-01 00:00:00"
    "stoptime": "2023-01-02 00:00:00", // REQUIRED: format "2023-01-02 00:00:00"
    "timezone": 15, // REQUIRED
    "timestep": 60, // REQUIRED
    "metrics_interval": 300, // DEFAULT: 300
    "WANT_VI_DUMP": false, // DEFAULT: false
    "random seed": 13, // DEFAULT: 1 - seeds randomizer used in populating the models

    "players": {"name": "lp","file": "your_file","type": "double"},

    // house parameters
    "use_house": true,
    "use_feeder": true,
    // use_feeder = true
    "taxonomy": "R1-12.47-1.glm",
    // else
    "region": 1,
    "vll": 12470.0,
    "vln": 7200.0,
    "avg_house": 4500.0,
    "avg_commercial": 30000.0,

    // add schedules
    "includes": [
        "${TESPDIR}/data/schedules/appliance_schedules.glm",
        "${TESPDIR}/data/schedules/water_and_setpoint_schedule_v5.glm",
        "${TESPDIR}/data/schedules/commercial_schedules.glm"
        ],

    // add sets in glm
    "set": [
        "minimum_timestep=15",
        "relax_naming_rules=1",
        "warn=0"
        ],

    // add defines
    "defines": [],

    // weather
    "latitude": 30.0, // DEFAULT:30.0
    "longitude": -110.0, // DEFAULT:-110.0
    "weather_name": "localWeather", // DEFAULT:localWeather
    "weather_file": "AZ-Tucson_International_Ap.tmy3", // DEFAULT:AZ-Tucson_International_Ap.tmy3

    "rooftop_pv_rating_MW": 1677.84989450533
}